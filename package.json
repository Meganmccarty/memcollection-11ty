{
  "name": "eleventy-starter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "run-p eleventy:dev tailwind:dev sass:dev js:dev js:dev:legacy",
    "build": "run-s eleventy:build tailwind:build sass:build js:build js:build:legacy",
    "eleventy:dev": "eleventy --serve",
    "eleventy:build": "eleventy",
    "tailwind:dev": "npx tailwindcss -i ./src/css/tw-styles.css -o ./dist/css/tw-styles.css --watch",
    "tailwind:build": "npx tailwindcss -i ./src/css/tw-styles.css -o ./dist/css/tw-styles.css --minify",
    "sass:dev": "sass --watch --embed-source-map src/scss:dist/css",
    "sass:build": "sass --no-source-map --style=compressed src/scss:dist/css",
    "clean": "rm -rf dist",
    "lint": "run-s lint:html lint:css lint:js",
    "pa11y-ci": "pa11y-ci -c .pa11yci.json './dist/**/*.html'",
    "lint:html": "npx start-server-and-test eleventy:dev 8080 pa11y-ci",
    "lint:css": "stylelint 'src/*css/**/*.*css' --color --formatter verbose",
    "js:dev": "esbuild src/js/*.js src/js/*.ts --format=esm --bundle=true --splitting=true --outdir=dist --watch --sourcemap=inline",
    "js:build": "esbuild src/js/*.js src/js/*.ts --format=esm --bundle=true --splitting=true --outdir=dist --minify=true",
    "js:dev:legacy": "NODE_ENV=development webpack --progress -w",
    "js:build:legacy": "NODE_ENV=production webpack --progress",
    "lint:js": "eslint . --ext .js,.ts",
    "test": "run-s test:jest test:cypress",
    "test:cypress": "npx cypress run",
    "test:jest": "jest --coverage",
    "update-deps": "ncu -u",
    "postupdate-deps": "npm install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "browserslist": [
    ">0%",
    "not dead",
    "not supports es6-module"
  ],
  "dependencies": {
    "@jest/globals": "^29.4.0",
    "cypress": "^12.3.0",
    "cypress-axe": "^1.3.0",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.2.6",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "axe-core": "^4.6.2",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.2",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.4",
    "eslint": "^8.32.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "npm-check-updates": "^16.6.3",
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^3.0.1",
    "start-server-and-test": "^1.15.3",
    "stylelint": "^14.16.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  }
}
