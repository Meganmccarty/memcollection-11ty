{
  "name": "mem-collection",
  "version": "0.3.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "run-p eleventy:dev sass:dev js:dev js:dev:legacy",
    "build": "run-s eleventy:build sass:build js:build js:build:legacy",
    "eleventy:dev": "eleventy --serve",
    "eleventy:build": "eleventy",
    "sass:dev": "sass --watch --embed-source-map src/scss:dist/css",
    "sass:build": "sass --no-source-map --style=compressed src/scss:dist/css",
    "clean": "rm -rf dist",
    "lint": "run-s lint:html lint:css lint:js",
    "pa11y-ci": "pa11y-ci -c .pa11yci.json -s http://localhost:8080/test-sitemap.xml --sitemap-find \"https://www.meganemccartycollection.com/\" --sitemap-replace \"http://localhost:8080/\"",
    "lint:html": "npx start-server-and-test eleventy:dev 8080 pa11y-ci",
    "lint:css": "stylelint 'src/*css/**/*.*css' --color --formatter verbose",
    "js:dev": "esbuild src/js/*.ts --format=esm --bundle=true --splitting=true --outdir=dist/js --watch --sourcemap=inline",
    "js:build": "esbuild src/js/*.ts --format=esm --bundle=true --splitting=true --outdir=dist/js --minify=true",
    "js:dev:legacy": "NODE_ENV=development webpack --progress -w",
    "js:build:legacy": "NODE_ENV=production webpack --progress",
    "lint:js": "eslint . --ext .js,.ts",
    "lint:js:fix": "eslint . --fix --ext .js,.ts",
    "test": "run-s test:jest test:cypress",
    "test:cypress": "npx cypress run",
    "test:jest": "jest --coverage --maxWorkers=1",
    "update-deps": "ncu -u",
    "postupdate-deps": "npm install"
  },
  "keywords": [],
  "author": "Megan McCarty",
  "license": "ISC",
  "browserslist": [
    ">0%",
    "not dead",
    "not supports es6-module"
  ],
  "dependencies": {
    "@11ty/eleventy-fetch": "^3.0.0",
    "@11ty/eleventy-server-browsersync": "^1.0.2",
    "@jest/globals": "^29.6.1",
    "cypress": "^12.17.2",
    "cypress-axe": "^1.3.0",
    "leaflet": "^1.9.4",
    "sass": "^1.69.6"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/jest": "^29.5.3",
    "@types/leaflet": "^1.9.3",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.62.0",
    "axe-core": "^4.6.2",
    "babel-jest": "^29.3.1",
    "babel-loader": "^9.1.2",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.4",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "npm-check-updates": "^16.11.1",
    "npm-run-all": "^4.1.5",
    "pa11y-ci": "^3.0.1",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^14.16.1",
    "stylelint-config-standard-scss": "^6.1.0",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.5.1",
    "typescript": "^4.9.4",
    "vercel": "^31.2.3",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  }
}
